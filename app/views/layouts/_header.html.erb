<ul>
  <% if logged_in?  %>
    <li><%= link_to "View my books", user_books_path(current_user) %></li>
    <li><%= link_to "Logout", session_path, method: :delete %></li>
  <% else %>
    <li><%= link_to "Sign-Up", new_user_path %></li>
    <li><%= link_to "Login", new_session_path %></li>
  <% end %>
</ul>
